{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "models/users.js",
    "models/unstructured.js",
    "models/slide.js",
    "models/service.js",
    "models/about.js",
    "models/product.js",
    "app/sitemap.xml/route.ts",
    "tailwind.config.js"
, "middlewares/backendMiddleware.ts", "app/api/v1/clients/route.js", "AdminComponents/mails/Mails.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
